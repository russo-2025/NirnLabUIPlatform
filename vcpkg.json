{
	"name": "nirnlab-ui-platform",
	"version-semver": "0.1.0",
	"description": "UI platform for Skyrim, including CEF (Chromium Embedded Framework) integration and more",
	"homepage": "https://github.com/kkEngine/NirnLabUIPlatform",
	"license": "MIT",
	"supports": "windows & x64",
	"dependencies": [
		{
			"name": "vcpkg-cmake-config",
			"host": true
		},
		{
			"name": "directxtk",
			"platform": "windows"
		},
		"palsigslot",
		"spdlog",
		"cef-prebuilt",
		"nlohmann-json"
	],
	"features": {
		"commonlib-se": {
			"description": "Build platform for Skyrim SE.",
			"dependencies": [
				{
					"name": "commonlib",
					"platform": "windows"
				}
			]
		},
		"commonlib-ae": {
			"description": "Build platform for Skyrim AE.",
			"dependencies": [
				{
					"name": "commonlib",
					"platform": "windows",
					"default-features": false,
					"features": ["support-ae"]
				}
			]
		}
	}
}
